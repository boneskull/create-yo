{
  "name": "create-yo",
  "version": "0.0.0-development",
  "description": "Use Yeoman generators via \"npm init\"",
  "main": "src/create-yo.js",
  "bin": {
    "create-yo": "src/create-yo.js"
  },
  "scripts": {
    "posttest": "eslint src test",
    "test": "mocha"
  },
  "keywords": [
    "yo",
    "yeoman",
    "npm",
    "init",
    "create",
    "scaffolding",
    "generator",
    "scaffold",
    "initialize",
    "npx"
  ],
  "author": "Christopher Hiller <boneskull@boneskull.com> (https://boneskull.com/)",
  "license": "Apache-2.0",
  "dependencies": {
    "ansi-bold": "^0.1.1",
    "global-dirs": "^1.0.0",
    "import-from": "^3.0.0",
    "log-symbols": "^3.0.0"
  },
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=6.1.0"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-config-semistandard": "^13.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^3.0.1",
    "lint-staged": "^9.2.0",
    "mocha": "^6.2.0",
    "prettier": "^1.18.2",
    "rewiremock": "^3.13.7",
    "semantic-release": "^15.13.18",
    "sinon": "^7.3.2",
    "unexpected": "^11.7.0",
    "unexpected-sinon": "^10.11.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/boneskull/create-yo.git"
  },
  "files": [
    "src/create-yo.js"
  ],
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.{yml,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "prettier": {
    "singleQuote": true,
    "bracketSpacing": false,
    "endOfLine": "auto"
  }
}
